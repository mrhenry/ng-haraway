{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "isolatedModules": false,
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noLib": false,
        "outDir": "lib",
        "preserveConstEnums": true,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./lib/**/*",
        "!./node_modules/**/*"
    ],
    "files": [
        "./src/chooser.ts",
        "./src/index.ts",
        "./src/inject.ts",
        "./src/module.ts",
        "./src/preview.ts",
        "./typings/angularjs/angular.d.ts",
        "./typings/jquery/jquery.d.ts"
    ]
}
